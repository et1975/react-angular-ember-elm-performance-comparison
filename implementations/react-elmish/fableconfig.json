{
    "sourceMaps": true,
    "projFile": "./todomvc.fsx",
    "outDir": "out",
//    "babelPlugins": ["transform-runtime"],
    "rollup": {
        "plugins": [
            ["replace", {"process.env.NODE_ENV": "'production'"}],
